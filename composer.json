{
  "name": "ricci69/phpprojectgen",
  "description": "Set all necessary components to create a skeleton project in PHP (composer) with PHPunit test cases",
  "type": "project",
  "require": {
    "phpunit/phpunit": "dev-main",
    "nelexa/zip": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "dev-main"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "ricci69",
      "email": "ricci69@protonmail.com"
    }
  ],
  "autoload": {
    "psr-0": {
      "ricci69\\PHPProjectGen\\": "src/",
      "ricci69\\PHPProjectGen\\Test\\": "tests/"
    },
    "psr-4": {
      "ricci69\\PHPProjectGen\\": "src/",
      "ricci69\\PHPProjectGen\\Test\\": "tests/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ricci69\\PHPProjectGen\\Test\\": "tests/"
    }
  },
  "minimum-stability": "dev"
}
